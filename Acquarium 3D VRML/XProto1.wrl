#VRML V2.0 utf8



PROTO FISH [field SFColor fishColor 1 1 0 #fish0
	  field SFColor finColor  1 1 0
    field SFColor specularColor 0.9 0.8 0.4  ] #PROTO creates new Node
   {
  Transform{
    rotation 1 0 0 1.57
		children[
			 DEF Flosse Shape{
			  	appearance Appearance {
					  material Material {
					 	diffuseColor IS fishColor
              
                    specularColor IS specularColor
			  	  	} 
			      	} # Seitenflossen
           geometry IndexedFaceSet{
              coord Coordinate{
               point[
                0 0 0  #0 linke bas
                1 0 0  #1 right base
                -0.25 1 0 #2 left breit
                1.25 1 0 #3 right breit
                0.5 2.5 0 #4 front end
                0.5 1 0.5 #5 front up
                0.5 1 -0.5 #6 front down 
                0.5 0 0 #7 middle beg
              ] 
              }
            coordIndex [0 2 5 7 -1 
            1 3 5 7 -1 
            2 4 5 -1
            3 5 4 -1 
            0 2 6 7 -1 
            1 3 6 7 -1                        
            2 4 6 -1 
            3 6 4 -1 

            ] solid FALSE
	    	} #IndexedFaceSet
  	  }#shape 
      Transform{  #spigelung flosse
     translation  1 -2 0
     rotation 0 0 1 3.14
        children[
          USE Flosse
     ]
        }

				

       DEF Body Shape{   #body
            appearance Appearance {
						material Material {
							diffuseColor IS finColor
            
                    specularColor IS specularColor
						} 
					}
        geometry IndexedFaceSet{
        coord Coordinate{
          point [
          #seiten
            3 0 -0.5 #0  down back 
            3 0 0.5 #1  up back
            -2 0 -0.5 #2  down front
            -2 0 0.5 #3  up front
         #obere teil   
            3 -1 1.5 #4 mid back
            -2 -1 1.5 #5 mid front
            0.5 -1 2 #6 mid mid
            0.5 0 1 #7 mid flo 
         #untere teil 
            0.5 0 -1 # 8 mid flo 
            3 -1 -1.5 #9 mid back
           -2  -1 -1.5 #10 mid front
            0.5 -1 -2 #11 mid mid
             #beg end
            
                    ]
        }
        coordIndex[0 1  7 3 2 8 -1 #seite
                   1 4 6 7 -1 #oben hinten
                   3 5 6 7 -1  #oben vorne
                   0 9 11 8 -1 #unten hinten
                   2 10 11 8 -1 #unten vorne
                  
                   ]solid FALSE
                }

       }
  Transform{ #spiegelung body
    translation  1 -2 0
    rotation 0 0 1 3.14
       children[
         USE Body
       ]
       
}
   DEF Kopf Shape{
        appearance Appearance {
						material Material {
							diffuseColor IS fishColor
              
                    specularColor IS specularColor 
						} 
					}
        geometry IndexedFaceSet{
            coord Coordinate{
              point [
                -2 0 -0.5 #0  down front
                -2 0 0.5 #1  up front
                -2 -1 1.5 #2 mid front
                -2  -1 -1.5 #3 mid front
                -5 -1 0 #4 kopf punkt
                -2 -2 -0.5 #5links untmid
                -2 -2 0.5 #6 links höher

                  ]
            }
            coordIndex[  4 2 1 -1 
            4 1 0 -1
            4 0 3 -1 
            4 3 5 -1
            4 5 6 -1
            4 6 2 -1 #kopf
                       
                       ]solid FALSE
        }
        
       
       }  

     DEF Rumpf Shape{
      appearance Appearance {
        material Material {
            diffuseColor IS fishColor
        
                    specularColor IS specularColor
          } 

        }
      geometry IndexedFaceSet{
        coord Coordinate{
          point [
          #ende 
           4.5 -0.75 0.25  #0 obere ecke right  
            4.5 -0.75 -0.25 #1untere ecke right
            4.5 -1 0.25 #2 mid up
            4.5 -1 -0.25 #3  mid down
            4.5 -1.25 0.25 #4 obere ecke right
            4.5 -1.25 -0.25 #5 untere ecke left
            #stuck
           5.5 -0.75 0.25  #6 obere ecke right  
            5.5 -0.75 -0.25 #7 untere ecke right
            5.5 -1.25 0.25 #8 obere ecke left
            5.5 -1.25 -0.25 #9 untere ecke left
            # ecken esagon 
            3 0 -0.5 #10  down right
            3 0 0.5 #11  up  right
            3 -1 -1.5 #12 mid  unten
           3 -1 1.5 #13 mid  oben 
           3 -2 -0.5 #14 down left
           3 -2 0.5   #15 up left
            
            #rumpf
            7 -1 2 #16 obere pinne
            7 -1 -2 #17 untere pinne
            
          ]
         }
        coordIndex[#ende
                   1 12 5 -1 #down mid
                   12 14 5 -1 # down left
                   4 5 14 15 -1 #mid left
                   4 15 13 -1 #up left
                   13 0 4 -1 #up mid
                   0 11 13 -1 #up right
                   0 11 10 1 -1 #mid right
                   1 10 12 -1 #down right
                   #stück
                   0 6 7 1 -1 #left 
                   8 4 5 9 -1 # right
                   0 6 8 4 -1 #up
                   1 5 9 7 -1 #down
                  #pinne
                  8 16 6 -1 #up
                  8 16 17 9 -1 #left
                  9 17 7 -1 #down
                  6 7 17 16 -1 #right

                   ]solid FALSE
    }

      }
                       
     ]
       }
 }
